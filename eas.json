{
  "cli": {
    "version": ">= 15.0.12",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "enterpriseProvisioning": "adhoc"
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_PUBLIC_RC_IOS": "appl_mTUgtfbBKJrxrnpaLDOUEPZiXcN"
      }
    },
    "simulator": {
      "ios": {
        "simulator": true
      }
    },
    "ipa-build": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "enterpriseProvisioning": "adhoc",
        "credentialsSource": "remote"
      },
      "env": {
        "EXPO_PUBLIC_RC_IOS": "appl_mTUgtfbBKJrxrnpaLDOUEPZiXcN"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
